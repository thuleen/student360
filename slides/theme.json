{
  "document": {
    "block_prefix": "\n",
    "block_suffix": "\n",
    "color": "252", 
    "margin": 2
  },
  "author": {
    "color": "142",  
    "bold": true,
    "prefix": "by ",
    "suffix": ""
  },
  "block_quote": {
    "indent": 1,
    "indent_token": "| ",
    "color": "243"  
  },
  "paragraph": {},
  "list": {
    "level_indent": 4,
    "color": "252"  
  },
  "heading": {
    "block_suffix": "\n",
    "color": "#fbf1c7",  
    "bold": true
  },
  "h1": {
    "prefix": "",
    "suffix": "",
    "color": "#FFDE21", 
    "bold": true
  },
  "h2": {
    "prefix": " ",
    "color": "#b8bb26" 
  },
  "h3": {
    "prefix": "### ",
    "color": "#fabd2f" 
  },
  "h4": {
    "prefix": "#### ",
    "color": "#fe8019"  
  },
  "h5": {
    "prefix": "##### ",
    "color": "#d65d0e" 
  },
  "h6": {
    "prefix": "###### ",
    "color": "#9d0006"  
  },
  "text": {},
  "strikethrough": {
    "block_prefix": "~~",
    "block_suffix": "~~"
  },
  "emph": {
    "block_prefix": "",
    "block_suffix": "",
    "style": "italic"
  },
  "strong": {
    "block_prefix": "",
    "block_suffix": "",
    "color": "#ffffff",
    "bold": true  
  },
  "hr": {
    "format": "\n--------\n",
    "color": "235"  
  },
  "item": {
    "block_prefix": "• ",
    "color": "252" 
  },
  "enumeration": {
    "block_prefix": ". ",
    "color": "252" 
  },
  "task": {
    "ticked": "[x] ",
    "unticked": "[ ] ",
    "color": "252"
  },
  "link": {
    "color": "#83a598" 
  },
  "link_text": {
    "color": "#83a598"  
  },
  "image": {},
  "image_text": {
    "format": "Image: {{.text}} →"
  },
  "code": {
    "block_prefix": "`",
    "block_suffix": "`",
    "color": "#d79921" 
  },
  "code_block": {
    "margin": 2,
    "background_color": "234", 
    "color": "252"  
  },
  "table": {},
  "definition_list": {},
  "definition_term": {},
  "definition_description": {
    "block_prefix": "\n* "
  },
  "html_block": {},
  "html_span": {}
}
